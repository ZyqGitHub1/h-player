    "got": "^9.6.0",
    "hls.js": "^0.12.4",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-pagination-2": "^2.0.3",
    "vue-plyr": "^5.1.3",
    "vue-router": "^3.0.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^18.3.7",
    "electron-icon-builder": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
